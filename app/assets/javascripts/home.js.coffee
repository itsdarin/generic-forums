$( ()->
  if $(".editor").length > 0
    console.log "Setting Window.Editors"
    window.editors = []
    $(".editor").each (i,v)->
      syntax = null
      $v = $(v)
      if $v.hasClass("editor_markdown")
        syntax = new MarkdownSyntax()
      window.editors.push new Editor($(v), syntax)
)
